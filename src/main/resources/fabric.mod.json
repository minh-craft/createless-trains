{
  "schemaVersion": 1,
  "id": "createless-trains",
  "version": "${version}",
  "name": "Create-less Trains",
  "description": "Make train accessories like the display board and train whistle usable independent of the rest of Create. Also remove non train related ponders.",
  "authors": [
    "minh-craft"
  ],
  "contact": {
    "homepage": "https://github.com/minh-craft/createless-trains"
  },
  "license": "MPL 2.0",
  "icon": "assets/createless-trains/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "com.minhtyfresh.createlesstrains.CreatelessTrains"
    ]
  },
  "mixins": [
    "createless-trains.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${fabric_loader_version}",
    "fabric-api": ">=${fabric_api_version}",
    "create": "${create_version}",
    "minecraft": "${minecraft_version}"
  }
}
